{
  "name": "github-coding-agent-pocs",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "dev:profile-display": "pnpm --filter profile-display dev",
    "dev:profile-edit": "pnpm --filter profile-edit dev",
    "build:profile-display": "pnpm --filter profile-display build",
    "build:profile-edit": "pnpm --filter profile-edit build",
    "build:all": "pnpm -r build",
    "dev:all": "pnpm -r --parallel dev"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "typescript": "^5.6.3"
  }
}